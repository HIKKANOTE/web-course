  TForm30 = class(TForm)
    Button1: TButton;
    Button2: TButton;
    procedure Button1Click(Sender: TObject);
    procedure Button2Click(Sender: TObject);

  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  Form30: TForm30;

implementation

{$R *.dfm}

procedure TForm30.Button1Click(Sender: TObject);
begin
close;
end;
procedure TForm30.Button2Click(Sender: TObject);
begin



ShellExecute(Handle, 'open', PChar(ExtractFilePath(Application.ExeName)+'\Диплом\прога\Видеоуроки\видео.avi'), nil, nil, SW_SHOWMAXIMIZED);
end;

